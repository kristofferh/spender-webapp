{
    "presets": [
      [
        "@babel/preset-env", {
          "targets": {
            "browsers": ["last 2 versions"]
          },
          "loose": true,
          "useBuiltIns": "usage",
          "shippedProposals": true,
          "modules": false
        }
      ],
      "@babel/preset-react"
    ],
    "plugins": [["emotion", { "sourceMap": true, "autoLabel": true }], "@babel/plugin-proposal-class-properties"],
    "env": {
      "test": {
        "presets": [
          [
            "@babel/preset-env", {
              "modules": "commonjs",
              "useBuiltIns": "usage",
              "debug": false
            }
          ]
        ]
      },
      "server": {
        "presets": [
          [
            "@babel/preset-env", {
              "targets": {
                "node": "current"
              },
              "modules": "commonjs",
              "useBuiltIns": "usage",
              "shippedProposals": true,
            }
          ]
        ]
      },
      "production": {
        "plugins": [["emotion", { "hoist": true }], "@babel/plugin-proposal-class-properties"],
      }
    }
}
