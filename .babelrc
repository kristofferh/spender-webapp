{
    "presets": [
    [
        "@babel/preset-env", {
        "targets": {
            "browsers": ["last 2 versions"]
        },
        "loose": true,
        "useBuiltIns": "usage",
        "shippedProposals": true,
        "modules": false
        }
    ],
        "@babel/preset-react"
    ],
    "plugins": ["@babel/plugin-proposal-class-properties"],
    "env": {
        "test": {
            "presets": [
              [
                "@babel/preset-env", {
                  "modules": "commonjs",
                  "useBuiltIns": "usage",
                  "debug": false
                }
              ]
            ]
        }
    }
}
