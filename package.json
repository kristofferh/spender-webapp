{
  "name": "framework-webapp",
  "version": "0.0.1",
  "description": "Framework starter web application",
  "main": "index.js",
  "repository": "git@github.com:kristofferh/framework-webapp.git",
  "author": "Kristoffer Hedstrom <kris.hedstrom@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "eslint src && jest",
    "dev": "webpack-dev-server --open --config ./webpack.config.dev.js",
    "build": "webpack --colors --progress --config ./webpack.config.prod.js",
    "local-dev":
      "webpack-dev-server --public local.dev:8080 --host 0.0.0.0 --config ./webpack.config.dev.js"
  },
  "browserslist": ["last 2 versions"],
  "jest": {
    "testEnvironment": "node",
    "verbose": true,
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "roots": ["__tests__/"],
    "modulePaths": ["src"],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    }
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "history": "^4.7.2",
    "isomorphic-fetch": "^2.2.1",
    "moment": "^2.20.1",
    "normalize.css": "^8.0.0",
    "numeral": "^2.0.6",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-select": "^1.0.1",
    "redux": "^3.7.2",
    "redux-form": "^7.1.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-es2015-parameters": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.10.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^21.2.1",
    "jest-cli": "^21.2.1",
    "node-sass": "^4.6.0",
    "postcss-loader": "^2.0.8",
    "prettier": "^1.7.4",
    "react-test-renderer": "^16.1.1",
    "redux-devtools-extension": "^2.13.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "styled-components": "^3.2.6",
    "webpack": "^4.8.0",
    "webpack-bundle-analyzer": "^2.11.2",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.4"
  }
