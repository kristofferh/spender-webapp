{
  "name": "spender-webapp",
  "version": "0.0.2",
  "description": "Spender web application",
  "main": "index.js",
  "repository": "git@github.com:kristofferh/spender-webapp.git",
  "author": "Kristoffer Hedstrom <kris.hedstrom@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">= 11.10.1"
  },
  "scripts": {
    "eslint": "eslint src",
    "test": "yarn eslint && yarn types && yarn jstest",
    "jstest": "jest",
    "jstest:output": "jest --json --outputFile=jest-test-results.json || true",
    "jstest:watch": "yarn jstest --watch",
    "jstest:output:watch": "jest --watch --json --outputFile=jest-test-results.json",
    "ci:before": "greenkeeper-lockfile-update",
    "ci:after": "greenkeeper-lockfile-upload",
    "types": "tsc",
    "types:watch": "yarn types -- --watch",
    "dev": "webpack-dev-server --open --config ./webpack.config.dev.js",
    "build": "BABEL_ENV=production webpack --colors --progress --config ./webpack.config.prod.js",
    "build:server": "BABEL_ENV=server babel src/index.js -d dist",
    "local:dev": "webpack-dev-server --public local.dev:8080 --host 0.0.0.0 --config ./webpack.config.dev.js",
    "start": "node dist/index.js",
    "heroku-postbuild": "yarn build && yarn build:server",
    "storybook": "yarn jstest:output && start-storybook"
  },
  "sideEffects": [
    "*.scss"
  ],
  "browserslist": [
    "last 2 versions",
    "not dead"
  ],
  "jest": {
    "verbose": true,
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "roots": [
      "__tests__/"
    ],
    "modulePaths": [
      "src"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/jest-setup.js"
    ]
  },
  "dependencies": {
    "@emotion/core": "^10.0.5",
    "@emotion/styled": "^10.0.5",
    "@vx/gradient": "^0.0.165",
    "@vx/grid": "^0.0.179",
    "@vx/scale": "^0.0.179",
    "@vx/shape": "^0.0.179",
    "classnames": "^2.2.5",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "formik": "^2.0.6",
    "history": "^4.7.2",
    "isomorphic-fetch": "^2.2.1",
    "js-cookie": "^2.2.0",
    "moment": "^2.20.1",
    "normalize.css": "^8.0.1",
    "numeral": "^2.0.6",
    "prop-types": "^15.6.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.1.0",
    "react-router-dom": "^5.0.0",
    "react-select": "^2.3.0",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.13.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.7.4",
    "@storybook/addon-jest": "^5.3.0",
    "@storybook/addon-knobs": "^5.3.0",
    "@storybook/react": "^5.3.0",
    "@types/jest": "^24.0.12",
    "@types/react": "^16.8.19",
    "@types/react-dom": "^16.8.4",
    "@types/react-redux": "^7.1.1",
    "@types/react-test-renderer": "^16.8.1",
    "autoprefixer": "^9.5.1",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-emotion": "^10.0.5",
    "css-loader": "^3.0.0",
    "cssnano": "^4.1.8",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-emotion": "^10.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.14.3",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.7.1",
    "jest-cli": "^24.7.1",
    "jest-emotion": "^10.0.5",
    "jest-fetch-mock": "^3.0.1",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.19.1",
    "react-test-renderer": "^16.8.6",
    "redux-devtools-extension": "^2.13.7",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "typescript": "^3.5.3",
    "webpack": "^4.36.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.10.1"
  }
}
