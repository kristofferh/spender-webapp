{
  "name": "spender-webapp",
  "version": "0.0.2",
  "description": "Spender web application",
  "main": "index.js",
  "repository": "git@github.com:kristofferh/spender-webapp.git",
  "author": "Kristoffer Hedstrom <kris.hedstrom@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">= 10.0.0"
  },
  "scripts": {
    "test": "eslint src && jest",
    "ci:before": "greenkeeper-lockfile-update",
    "ci:after": "greenkeeper-lockfile-upload",
    "dev": "webpack-dev-server --open --config ./webpack.config.dev.js",
    "build": "BABEL_ENV=production webpack --colors --progress --config ./webpack.config.prod.js",
    "build-server": "BABEL_ENV=server babel src/index.js -d dist",
    "local-dev": "webpack-dev-server --public local.dev:8080 --host 0.0.0.0 --config ./webpack.config.dev.js",
    "start": "node dist/index.js",
    "heroku-postbuild": "yarn build && yarn build-server"
  },
  "sideEffects": [
    "@babel/polyfill",
    "*.scss"
  ],
  "browserslist": [
    "last 2 versions"
  ],
  "jest": {
    "verbose": true,
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "roots": [
      "__tests__/"
    ],
    "modulePaths": [
      "src"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/jest-setup.js"
  },
  "dependencies": {
    "@emotion/core": "^10.0.5",
    "@emotion/styled": "^10.0.5",
    "@vx/gradient": "^0.0.165",
    "@vx/grid": "^0.0.179",
    "@vx/scale": "^0.0.179",
    "@vx/shape": "^0.0.179",
    "classnames": "^2.2.5",
    "compression": "^1.7.2",
    "express": "^4.16.3",
    "history": "^4.7.2",
    "isomorphic-fetch": "^2.2.1",
    "js-cookie": "^2.2.0",
    "moment": "^2.20.1",
    "normalize.css": "^8.0.1",
    "numeral": "^2.0.6",
    "prop-types": "^15.6.0",
    "query-string": "^6.2.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.1.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-select": "^2.3.0",
    "redux": "^4.0.0",
    "redux-form": "^7.4.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.4.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-emotion": "^10.0.5",
    "css-loader": "^2.1.0",
    "cssnano": "^4.1.8",
    "eslint": "^5.6.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-emotion": "^10.0.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.7.1",
    "jest-cli": "^24.7.1",
    "jest-emotion": "^10.0.5",
    "jest-fetch-mock": "^2.1.0",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.17.0",
    "react-test-renderer": "^16.7.0",
    "redux-devtools-extension": "^2.13.7",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.23.1",
    "webpack": "^4.28.2",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14"
  }
}
