{
  "name": "spender-webapp",
  "version": "0.0.2",
  "description": "Spender web application",
  "main": "index.js",
  "repository": "git@github.com:kristofferh/spender-webapp.git",
  "author": "Kristoffer Hedstrom <kris.hedstrom@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">= 11.10.1"
  },
  "scripts": {
    "eslint": "eslint src",
    "test": "yarn eslint && yarn types && yarn jstest",
    "jstest": "jest",
    "jstest:output": "jest --json --outputFile=jest-test-results.json || true",
    "jstest:watch": "yarn jstest --watch",
    "jstest:output:watch": "jest --watch --json --outputFile=jest-test-results.json",
    "types": "tsc",
    "types:watch": "yarn types -- --watch",
    "dev": "webpack serve --open --config ./webpack.config.dev.js",
    "build": "BABEL_ENV=production webpack --progress --config ./webpack.config.prod.js",
    "build:server": "BABEL_ENV=server babel src/index.js -d dist",
    "local:dev": "webpack serve --public local.dev:8080 --host 0.0.0.0 --config ./webpack.config.dev.js",
    "start": "node dist/index.js",
    "heroku-postbuild": "yarn build && yarn build:server",
    "storybook": "yarn jstest:output && start-storybook"
  },
  "sideEffects": [
    "*.scss"
  ],
  "browserslist": [
    "last 2 versions",
    "not dead"
  ],
  "jest": {
    "verbose": true,
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "roots": [
      "__tests__/"
    ],
    "modulePaths": [
      "src"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/jest-setup.js"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@kristofferh/businesskit": "^0.0.2",
    "@vx/gradient": "^0.0.165",
    "@vx/grid": "^0.0.179",
    "@vx/scale": "^0.0.179",
    "@vx/shape": "^0.0.179",
    "classnames": "^2.2.5",
    "compression": "^1.7.4",
    "express": "^4.17.3",
    "formik": "^2.1.4",
    "history": "^4.7.2",
    "js-cookie": "^2.2.0",
    "mathjs": "^7.1.0",
    "moment": "^2.20.1",
    "normalize.css": "^8.0.1",
    "numeral": "^2.0.6",
    "polished": "^3.5.2",
    "prop-types": "^15.6.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-dropzone": "^10.2.2",
    "react-helmet": "^5.2.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "react-select": "^2.3.0",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.13.2",
    "uuid": "^7.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.7.4",
    "@emotion/babel-preset-css-prop": "^11.2.0",
    "@emotion/eslint-plugin": "^11.2.0",
    "@emotion/jest": "^11.3.0",
    "@storybook/addon-jest": "^5.3.0",
    "@storybook/addon-knobs": "^5.3.0",
    "@storybook/react": "^5.3.0",
    "@types/classnames": "^2.2.9",
    "@types/jest": "^24.0.12",
    "@types/js-cookie": "^2.2.6",
    "@types/mathjs": "^6.0.5",
    "@types/react": "^16.9.33",
    "@types/react-dom": "^16.9.6",
    "@types/react-helmet": "^5.0.15",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.6",
    "@types/react-test-renderer": "^16.8.1",
    "autoprefixer": "^10.2.6",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.0.5",
    "babel-plugin-emotion": "^11.0.0",
    "css-loader": "^5.2.6",
    "cssnano": "^5.0.5",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "jest-fetch-mock": "^3.0.1",
    "postcss": "^8.3.0",
    "postcss-loader": "^5.3.0",
    "prettier": "^2.3.0",
    "react-test-renderer": "^16.14.0",
    "redux-devtools-extension": "^2.13.7",
    "sass": "^1.34.0",
    "sass-loader": "^11.1.1",
    "style-loader": "^2.0.0",
    "typescript": "^4.0.5",
    "webpack": "^5.38.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2"
  }
}
